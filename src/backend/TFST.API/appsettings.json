{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FeatureFlags": {
    "MigrateAtStartup": false
  },
  "Cors": {
    "AllowedOrigins": []
  },
  "JwtSettings": {
    "Issuer": "SetInEnvironmentOrSecrets",
    "Audience": "SetInEnvironmentOrSecrets"
  },
  "OpenIddict": {
    "Issuer": "SetInEnvironmentOrSecrets",
    "Audience": "SetInEnvironmentOrSecrets"
  },
  "AdminSettings": {
    "Email": "admin@example.com",
    "FirstName": "Admin",
    "LastName": "User",
    "Roles": [ "Admin", "User" ]
  }
}
